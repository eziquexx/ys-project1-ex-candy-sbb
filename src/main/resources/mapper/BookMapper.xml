<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ys.project1.ex.sbb.BookMapper">
	<select id="getBook" resultType="com.ys.project1.ex.sbb.DTO.BookDTO">
		SELECT * FROM book WHERE id = #{id} 
	</select>
	<insert id="insertInfo" parameterMap="com.ys.project1.ex.sbb.DTO.BookDTO">
		INSERT INTO book (name, author) values (#{name}, #{author})
	</insert>
</mapper>